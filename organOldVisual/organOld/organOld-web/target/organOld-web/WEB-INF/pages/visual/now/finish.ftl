                                                                    function array_remove_repeat(a) { // 去重
                                                                        var r = [];
                                                                        for(var i = 0; i < a.length; i ++) {
                                                                            var flag = true;
                                                                            var temp = a[i];
                                                                            for(var j = 0; j < r.length; j ++) {
                                                                                if(temp === r[j]) {
                                                                                    flag = false;
                                                                                    break;
                                                                                }
                                                                            }
                                                                            if(flag) {
                                                                                r.push(temp);
                                                                            }
                                                                        }
                                                                        return r;
                                                                    }
                                                                    function listIntersection(x, y) {
                                                                        var result = [];
                                                                        for (var i = 0; i < y.length; i++) {
                                                                            var temp = y[i];
                                                                            for (var j = 0; j < x.length; j++) {
                                                                                // 普通数组 (temp === clone[j])
                                                                                if (temp.id === x[j].id) {
                                                                                    result.push(temp);
                                                                                    break;
                                                                                }
                                                                            }
                                                                        }
                                                                        return array_remove_repeat(result);
                                                                    }


